// Place your settings in this file to overwrite default and user settings.
{
    "cSpell.ignorePaths": [
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/**",
        "package.json",
        "**/cspell.json",
        "**/cSpell.json",
        "**/.cspell.json",
        "dictionaries",
        ".vscode",
        "typings"
